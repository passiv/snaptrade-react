{
  "name": "snaptrade-react",
  "version": "0.0.0",
  "description": "A React component for SnapTrade connection portal.",
  "license": "MIT",
  "author": {
    "name": "Shayan Abedi",
    "email": "shayan.abedi@passiv.com",
    "url": "https://shayanabedi.com"
  },
  "keywords": [
    "react",
    "react-component",
    "passiv",
    "snaptrade",
    "snaptradereact",
    "snaptradesdk"
  ],
  "files": [
    "dist"
  ],
  "main": "./dist/snaptrade-react.umd.js",
  "module": "./dist/snaptrade-react.es.js",
  "exports": {
    ".": {
      "import": "./dist/snaptrade-react.es.js",
      "require": "./dist/snaptrade-react.umd.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc --emitDeclarationOnly",
    "preview": "vite preview"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true,
      "es2020": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "parserOptions": {
      "sourceType": "module"
    }
  },
  "dependencies": {
    "@reach/dialog": "^0.17.0",
    "@reach/portal": "^0.17.0",
    "@reach/visually-hidden": "^0.17.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^18.6.1",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.0.0",
    "typescript": "^4.6.4",
    "vite": "^3.0.0"
  },
  "tags": [
    "react",
    "snaptrade",
    "passiv"
  ]
}
